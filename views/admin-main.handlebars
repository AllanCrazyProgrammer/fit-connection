<div class="container-top">
  <a href="/admin/usuarios"><img src="/images/btn-usuarios.png"></a>
  
  <a href="/admin/servicios"><img src="/images/btn-servicios.png"></a>

  <div class="text-inicio">
    <img src="/images/user-green.png">
    <h1>Usuarios - Vencimientos</h1>
    <table class="table table-dark">
      <thead>
        <tr>
          <th scope="col">Id</th>
          <th scope="col">Id servicio</th>
          <th scope="col">Fecha a vencer</th>


        </tr>
      </thead>

      {{#each mainTable}}

      <tr class="col fila-{{id}}">
        <td contenteditable="true" class="nombre">{{UsuarioId}}</td>
        <td contenteditable="true" class="correo">{{ServicioId}}</td>
        <td contenteditable="true" class="telefono">{{fechaFinal}}</td>
      </tr>
      {{/each}}

    </table>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>